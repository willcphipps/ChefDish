@model List<Dish>

<div class="m-2 float-right">
    <h3><a href="/AddDish">Add A Dish</a></h3>
</div>
<div class="m-5 text-center">
    <h1><a href="/">Chefs</a>  |  Dishes</h1>
</div>
    <div class="col-lg-12">
        <h3>Dishes</h3>
        <table class="table table-striped table-inverse">
            <thead class="thead-inverse">
                <tr>
                    <th>Name of Dish</th>
                    <th>Tastiness</th>
                    <th>Calories</th>
                    <th>Chef Name</th>
                </tr>
                </thead>
                <tbody>
                    @foreach(var dish in Model){
                    <tr>
                        <td scope="row"><a href="/destroy/@dish.DishId">@dish.DishName</a></td>
                        <td>@dish.Tastiness</td>
                        <td>@dish.Calories</td>
                        <td>@dish.Creator.FirstName the @dish.Creator.CatType </td>
                    </tr>
                    }
                </tbody>
        </table>
    </div>